{
  "name": "coc-reveal",
  "version": "0.1.0",
  "description": "Transform your Markdown to slides",
  "author": "Gerald <gera2ld@live.com>",
  "license": "MIT",
  "scripts": {
    "ci": "run-s lint",
    "lint": "eslint --ext .ts,tsx src",
    "dev": "rollup -wc rollup.conf.js",
    "clean": "del-cli dist types",
    "build:js": "rollup -c rollup.conf.js",
    "build": "run-s ci clean build:types build:js",
    "postinstall": "husky install",
    "pinst": "pinst",
    "prepublishOnly": "run-s build 'pinst -- --disable'",
    "postpublish": "pinst --enable",
    "build:types": "tsc -p tsconfig.prod.json"
  },
  "main": "dist/index.common.js",
  "files": [
    "dist",
    "types"
  ],
  "engines": {
    "coc": ">= 0.0.70"
  },
  "keywords": [
    "coc.nvim",
    "reveal"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "typings": "types/index.d.ts",
  "dependencies": {
    "js-yaml": "^4.0.0",
    "open": "^8.0.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.13.10",
    "@gera2ld/plaid": "~2.2.2",
    "@gera2ld/plaid-common-ts": "~2.2.0",
    "@gera2ld/plaid-rollup": "~2.2.0",
    "coc.nvim": "^0.0.80",
    "del-cli": "^3.0.1",
    "husky": "^5.1.3",
    "pinst": "^2.1.6"
  },
  "repository": "git@github.com:gera2ld/coc-reveal.git"
}
